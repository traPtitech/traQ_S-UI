{
  "name": "traq_s-ui",
  "version": "3.16.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with-font": "npm run gen-fonts && npm run build",
    "build:may-with-font": "MAY_SKIP_FONT_GEN=1 npm run gen-fonts && npm run build",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest run --coverage",
    "test:unit:watch": "vitest -w",
    "test:e2e": "npm run build && npm run test:e2e:only-run",
    "test:e2e-headless": "npm run build:may-with-font && npm run test:e2e:only-run-headless",
    "test:e2e:only-run": "start-server-and-test preview 4173 'cypress open --e2e'",
    "test:e2e:only-run-headless": "start-server-and-test preview 4173 'cypress run --headless'",
    "lint": "eslint --fix --cache .",
    "lint:no-fix": "eslint --cache .",
    "format": "prettier --cache --write .",
    "type-check": "npm run type-check:src && npm run type-check:test",
    "type-check:src": "vue-tsc --noEmit",
    "type-check:test": "tsc --noEmit --project tests/unit/tsconfig.json && tsc --noEmit --project tests/e2e/tsconfig.json",
    "gen-fonts": "node build/gen-mplus.js",
    "gen-unicode_emojis": "node build/gen-unicode_emojis.js",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@mdi/js": "^7.0.96",
    "@sapphi-red/web-noise-suppressor": "^0.3.1",
    "@traptitech/traq": "^3.11.0-3",
    "@traptitech/traq-markdown-it": "^6.2.1",
    "autosize": "^5.0.1",
    "cropperjs": "^1.5.12",
    "deepmerge": "^4.2.2",
    "dequal": "^2.0.3",
    "firebase": "^9.9.4",
    "highlight.js": "^11.6.0",
    "idb-keyval": "^6.2.0",
    "mitt": "^3.0.0",
    "pica": "^9.0.1",
    "skyway-js": "^4.4.5",
    "text-field-edit": "^3.1.9001",
    "throttle-debounce": "^5.0.0",
    "turndown": "^7.1.1",
    "turndown-plugin-gfm": "^1.0.2",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "vue-slider-component": "^4.0.0-beta.9",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.14",
    "@types/autosize": "^4.0.1",
    "@types/dom-screen-wake-lock": "^1.0.0",
    "@types/katex": "^0.14.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^16.18.3",
    "@types/pica": "^9.0.1",
    "@types/punycode": "^2.1.0",
    "@types/serviceworker": "^0.0.51",
    "@types/throttle-debounce": "^5.0.0",
    "@types/turndown": "^5.0.1",
    "@types/webappsec-credential-management": "^0.6.2",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@typescript/lib-dom": "npm:@types/web@^0.0.72",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.25.8",
    "@vue/test-utils": "^2.2.3",
    "autoprefixer": "^10.4.9",
    "axios": "^0.27.2",
    "browserslist": "^4.21.3",
    "cypress": "^12.1.0",
    "esbuild": "^0.15.7",
    "esbuild-plugin-browserslist": "^0.5.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.4.0",
    "fonteditor-core": "^2.1.10",
    "jsdom": "^20.0.0",
    "patch-package": "^6.4.7",
    "pinia": "^2.0.18",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "rollup-plugin-brotli": "^3.1.0",
    "sass": "^1.54.9",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.8.3",
    "vite": "^4.0.1",
    "vite-plugin-pwa": "^0.14.0",
    "vite-svg-loader": "^3.6.0",
    "vitest": "^0.25.8",
    "vitest-github-actions-reporter": "^0.9.0",
    "vue-tsc": "^1.0.17",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4"
  },
  "engines": {
    "node": "^16.17.0",
    "npm": "^8.5.0"
  }
}
