{
  "name": "traq_s-ui",
  "version": "3.15.3",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with-font": "npm run gen-fonts && npm run build",
    "build:may-with-font": "MAY_SKIP_FONT_GEN=1 npm run gen-fonts && npm run build",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest run --coverage",
    "test:unit:watch": "vitest -w",
    "test:e2e": "npm run build && npm run test:e2e:only-run",
    "test:e2e-headless": "npm run build:may-with-font && npm run test:e2e:only-run-headless",
    "test:e2e:only-run": "start-server-and-test preview 4173 'cypress open'",
    "test:e2e:only-run-headless": "start-server-and-test preview 4173 'cypress run --headless'",
    "lint": "eslint --fix --cache .",
    "lint:no-fix": "eslint --cache .",
    "type-check": "npm run type-check:src && npm run type-check:test",
    "type-check:src": "vue-tsc --noEmit",
    "type-check:test": "tsc --noEmit --project tests/unit/tsconfig.json && tsc --noEmit --project tests/e2e/tsconfig.json",
    "gen-fonts": "node build/gen-mplus.js",
    "gen-unicode_emojis": "node build/gen-unicode_emojis.js",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@mdi/js": "^6.6.96",
    "@sapphi-red/web-noise-suppressor": "^0.3.1",
    "@traptitech/traq": "^3.9.1-7",
    "@traptitech/traq-markdown-it": "^6.1.6",
    "autosize": "^5.0.1",
    "cropperjs": "^1.5.12",
    "deepmerge": "^4.2.2",
    "dequal": "^2.0.2",
    "firebase": "^9.6.10",
    "highlight.js": "^11.5.1",
    "idb-keyval": "^6.1.0",
    "mitt": "^3.0.0",
    "pica": "^9.0.1",
    "skyway-js": "^4.4.4",
    "text-field-edit": "^3.1.9001",
    "throttle-debounce": "^4.0.0",
    "turndown": "^7.1.1",
    "turndown-plugin-gfm": "^1.0.2",
    "vue": "^3.2.32",
    "vue-router": "^4.0.14",
    "vue-slider-component": "^4.0.0-beta.5",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.11",
    "@types/autosize": "^4.0.1",
    "@types/dom-screen-wake-lock": "^1.0.0",
    "@types/katex": "^0.14.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^16.11.26",
    "@types/pica": "^9.0.0",
    "@types/punycode": "^2.1.0",
    "@types/serviceworker": "^0.0.41",
    "@types/throttle-debounce": "^4.0.0",
    "@types/turndown": "^5.0.1",
    "@types/webappsec-credential-management": "^0.6.2",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@typescript/lib-dom": "npm:@types/web@^0.0.60",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/test-utils": "^2.0.0-rc.19",
    "autoprefixer": "^10.4.4",
    "axios": "^0.26.1",
    "browserslist": "^4.20.2",
    "c8": "^7.11.0",
    "cypress": "^9.5.4",
    "esbuild": "^0.14.36",
    "esbuild-plugin-browserslist": "^0.4.9",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "fonteditor-core": "^2.1.8",
    "jsdom": "^19.0.0",
    "patch-package": "^6.4.7",
    "pinia": "^2.0.13",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "rollup-plugin-brotli": "^3.1.0",
    "sass": "^1.50.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vite-plugin-pwa": "^0.11.13",
    "vite-svg-loader": "^3.2.0",
    "vitest": "^0.9.3",
    "vitest-github-actions-reporter": "^0.3.2",
    "vue-tsc": "^0.34.5",
    "workbox-cacheable-response": "^6.5.2",
    "workbox-core": "^6.5.2",
    "workbox-expiration": "^6.5.2",
    "workbox-precaching": "^6.5.2",
    "workbox-routing": "^6.5.2",
    "workbox-strategies": "^6.5.2"
  },
  "overrides": {
    "fonteditor-core": {
      "xmldom": "npm:@xmldom/xmldom@^0.7.5"
    }
  },
  "engines": {
    "node": "^16.14.2",
    "npm": "^8.5.0"
  }
}
