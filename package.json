    "test:e2e:only-run": "cypress open",
    "test:e2e:only-run-headless": "cypress run --headless",
    "lint": "eslint --ext .ts,.vue --fix --cache .",
    "lint:no-fix": "eslint --ext .ts,.vue --cache .",
    "gen-fonts": "node build/gen-mplus.mjs",
    "gen-unicode_emojis": "node build/gen-unicode_emojis.mjs",
