{
  "name": "traq_s-ui",
  "version": "3.13.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with-font": "npm run gen-fonts && npm run build",
    "build:may-with-font": "MAY_SKIP_FONT_GEN=1 npm run gen-fonts && npm run build",
    "preview": "vite preview --port 5000",
    "test:unit": "jest",
    "test:e2e": "npm run build && npm run test:e2e:only-run",
    "test:e2e-headless": "npm run build:may-with-font && npm run test:e2e:only-run-headless",
    "test:e2e:only-run": "start-server-and-test preview 5000 'cypress open'",
    "test:e2e:only-run-headless": "start-server-and-test preview 5000 'cypress run --headless'",
    "lint": "eslint --fix --cache .",
    "lint:no-fix": "eslint --cache .",
    "type-check": "npm run type-check:src && npm run type-check:test",
    "type-check:src": "vue-tsc --noEmit",
    "type-check:test": "tsc --noEmit --project tests/unit/tsconfig.json && tsc --noEmit --project tests/e2e/tsconfig.json",
    "gen-fonts": "node build/gen-mplus.mjs",
    "gen-unicode_emojis": "node build/gen-unicode_emojis.mjs",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@mdi/js": "^6.5.95",
    "@sapphi-red/dtln-web": "^0.1.3",
    "@traptitech/traq": "^3.8.2-3",
    "@traptitech/traq-markdown-it": "^6.1.2",
    "autosize": "^5.0.1",
    "cropperjs": "^1.5.12",
    "deepmerge": "^4.2.2",
    "dequal": "^2.0.2",
    "direct-vuex": "^0.12.1",
    "firebase": "^9.6.2",
    "highlight.js": "^11.4.0",
    "idb-keyval": "^6.0.3",
    "klona": "^2.0.5",
    "mitt": "^2.1.0",
    "pica": "^7.1.0",
    "shvl": "^2.0.3",
    "skyway-js": "^4.4.3",
    "text-field-edit": "^3.1.2022",
    "throttle-debounce": "^3.0.1",
    "turndown": "^7.1.1",
    "turndown-plugin-gfm": "^1.0.2",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12",
    "vue-slider-component": "^4.0.0-beta.5",
    "vuex": "^4.0.2",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@types/autosize": "^4.0.1",
