{
  "name": "traq_s-ui",
  "version": "3.12.8",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with-font": "npm run gen-fonts && npm run build",
    "build:may-with-font": "MAY_SKIP_FONT_GEN=1 npm run gen-fonts && npm run build",
    "serve": "vite preview",
    "test:unit": "jest",
    "test:e2e": "npm run build && npm run test:e2e:without-build",
    "test:e2e-headless": "npm run build:may-with-font && npm run test:e2e:without-build -- --headless",
    "test:e2e:without-build": "node tests/e2e/run-cypress.mjs",
    "test:e2e:only-run": "cypress open",
    "test:e2e:only-run-headless": "cypress run --headless",
    "lint": "eslint --fix --cache .",
    "lint:no-fix": "eslint --cache .",
    "type-check": "npm run type-check:src && npm run type-check:test",
    "type-check:src": "vue-tsc --noEmit",
    "type-check:test": "tsc --noEmit --project tests/unit/tsconfig.json && tsc --noEmit --project tests/e2e/tsconfig.json",
    "gen-fonts": "node build/gen-mplus.mjs",
    "gen-unicode_emojis": "node build/gen-unicode_emojis.mjs",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@mdi/js": "^6.5.95",
    "@traptitech/traq": "^3.8.2-3",
    "@traptitech/traq-markdown-it": "^6.1.0",
    "autosize": "^5.0.1",
    "cropperjs": "^1.5.12",
    "deepmerge": "^4.2.2",
    "dequal": "^2.0.2",
    "direct-vuex": "^0.12.1",
    "firebase": "^9.3.0",
    "highlight.js": "^11.3.1",
    "idb-keyval": "^6.0.3",
    "klona": "^2.0.5",
    "mitt": "^2.1.0",
    "pica": "^7.1.0",
    "shvl": "^2.0.3",
    "skyway-js": "^4.4.3",
    "text-field-edit": "^3.1.1",
    "throttle-debounce": "^3.0.1",
    "turndown": "^7.1.1",
    "turndown-plugin-gfm": "^1.0.2",
    "vue": "^3.2.21",
    "vue-router": "^4.0.12",
    "vue-slider-component": "^4.0.0-beta.5",
    "vuex": "^4.0.2",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@types/autosize": "^4.0.0",
    "@types/dom-screen-wake-lock": "^1.0.0",
    "@types/jest": "^27.0.2",
    "@types/katex": "^0.11.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^16.11.7",
    "@types/pica": "^5.1.3",
    "@types/punycode": "^2.1.0",
    "@types/throttle-debounce": "^2.1.0",
    "@types/turndown": "^5.0.1",
    "@types/webappsec-credential-management": "^0.6.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.2.12",
    "@vue/test-utils": "^2.0.0-rc.16",
    "@vue/vue3-jest": "^27.0.0-alpha.3",
    "autoprefixer": "^10.4.0",
    "axios": "^0.24.0",
    "babel-jest": "^27.3.1",
    "browserslist": "^4.17.6",
    "cypress": "^9.0.0",
    "es-jest": "^1.4.1",
    "esbuild": "^0.13.13",
    "esbuild-plugin-browserslist": "^0.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "fonteditor-core": "^2.1.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-github-actions-reporter": "^1.0.3",
    "patch-package": "^6.4.7",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "rollup-plugin-brotli": "^3.1.0",
    "sass": "^1.43.4",
    "typescript": "^4.4.4",
    "vite": "^2.6.14",
    "vite-plugin-pwa": "^0.11.3",
    "vite-svg-loader": "^3.1.0",
    "vue-tsc": "^0.29.4",
    "workbox-cacheable-response": "^6.3.0",
    "workbox-core": "^6.3.0",
    "workbox-precaching": "^6.3.0",
    "workbox-routing": "^6.3.0",
    "workbox-strategies": "^6.3.0"
  },
  "engines": {
    "node": "^16.11.1",
    "npm": "^7.19.1"
  }
}
