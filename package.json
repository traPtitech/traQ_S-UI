{
  "name": "traq_s-ui",
  "version": "3.15.7",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with-font": "npm run gen-fonts && npm run build",
    "build:may-with-font": "MAY_SKIP_FONT_GEN=1 npm run gen-fonts && npm run build",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest run --coverage",
    "test:unit:watch": "vitest -w",
    "test:e2e": "npm run build && npm run test:e2e:only-run",
    "test:e2e-headless": "npm run build:may-with-font && npm run test:e2e:only-run-headless",
    "test:e2e:only-run": "start-server-and-test preview 4173 'cypress open --e2e'",
    "test:e2e:only-run-headless": "start-server-and-test preview 4173 'cypress run --headless'",
    "lint": "eslint --fix --cache .",
    "lint:no-fix": "eslint --cache .",
    "format": "prettier --cache --write .",
    "type-check": "npm run type-check:src && npm run type-check:test",
    "type-check:src": "vue-tsc --noEmit",
    "type-check:test": "tsc --noEmit --project tests/unit/tsconfig.json && tsc --noEmit --project tests/e2e/tsconfig.json",
    "gen-fonts": "node build/gen-mplus.js",
    "gen-unicode_emojis": "node build/gen-unicode_emojis.js",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@mdi/js": "^6.9.96",
    "@sapphi-red/web-noise-suppressor": "^0.3.1",
    "@traptitech/traq": "^3.10.0-2",
    "@traptitech/traq-markdown-it": "^6.2.0",
    "autosize": "^5.0.1",
    "cropperjs": "^1.5.12",
    "deepmerge": "^4.2.2",
    "dequal": "^2.0.2",
    "firebase": "^9.8.4",
    "highlight.js": "^11.5.1",
    "idb-keyval": "^6.2.0",
    "mitt": "^3.0.0",
    "pica": "^9.0.1",
    "skyway-js": "^4.4.4",
    "text-field-edit": "^3.1.9001",
    "throttle-debounce": "^5.0.0",
    "turndown": "^7.1.1",
    "turndown-plugin-gfm": "^1.0.2",
    "vue": "^3.2.37",
    "vue-router": "^4.1.1",
    "vue-slider-component": "^4.0.0-beta.9",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.12",
    "@types/autosize": "^4.0.1",
    "@types/dom-screen-wake-lock": "^1.0.0",
    "@types/katex": "^0.14.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^16.11.43",
    "@types/pica": "^9.0.1",
    "@types/punycode": "^2.1.0",
    "@types/serviceworker": "^0.0.48",
    "@types/throttle-debounce": "^5.0.0",
    "@types/turndown": "^5.0.1",
    "@types/webappsec-credential-management": "^0.6.2",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@typescript/lib-dom": "npm:@types/web@^0.0.60",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/test-utils": "^2.0.2",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "browserslist": "^4.21.1",
    "c8": "^7.11.3",
    "cypress": "^10.3.0",
    "esbuild": "^0.14.48",
    "esbuild-plugin-browserslist": "^0.4.12",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.2.0",
    "fonteditor-core": "^2.1.8",
    "jsdom": "^20.0.0",
    "patch-package": "^6.4.7",
    "pinia": "^2.0.14",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "rollup-plugin-brotli": "^3.1.0",
    "sass": "^1.53.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-plugin-pwa": "^0.12.3",
    "vite-svg-loader": "^3.4.0",
    "vitest": "^0.18.1",
    "vitest-github-actions-reporter": "^0.8.1",
    "vue-tsc": "^0.38.8",
    "workbox-cacheable-response": "^6.5.3",
    "workbox-core": "^6.5.3",
    "workbox-expiration": "^6.5.3",
    "workbox-precaching": "^6.5.3",
    "workbox-routing": "^6.5.3",
    "workbox-strategies": "^6.5.3"
  },
  "overrides": {
    "fonteditor-core": {
      "xmldom": "npm:@xmldom/xmldom@^0.7.5"
    }
  },
  "engines": {
    "node": "^16.16.0",
    "npm": "^8.5.0"
  }
}
