{
  "name": "traq_s-ui",
  "version": "3.31.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with-font": "npm run gen-fonts && npm run build",
    "build:may-with-font": "MAY_SKIP_FONT_GEN=1 npm run gen-fonts && npm run build",
    "preview": "vite preview --host 127.0.0.1 --port 4173",
    "test:unit": "vitest run --coverage",
    "test:unit:watch": "vitest -w",
    "test:e2e": "npm run build && npm run test:e2e:only-run",
    "test:e2e-headless": "npm run build:may-with-font && npm run test:e2e:only-run-headless",
    "test:e2e:only-run": "start-server-and-test preview 4173 'cypress open --e2e'",
    "test:e2e:only-run-headless": "start-server-and-test preview 4173 'cypress run --headless'",
    "lint": "eslint --fix --cache .",
    "lint:no-fix": "eslint --cache .",
    "format": "prettier --cache --write .",
    "type-check": "npm run type-check:src && npm run type-check:test",
    "type-check:src": "vue-tsc --noEmit",
    "type-check:test": "tsc --noEmit --project tests/unit/tsconfig.json && tsc --noEmit --project tests/e2e/tsconfig.json",
    "gen-fonts": "node build/gen-mplus.js",
    "gen-unicode_emojis": "node build/gen-unicode_emojis.js",
    "localhost-certs:gen": "./build/gen-localhost-certs.sh",
    "localhost-certs:rem": "rm -rf ./.certs",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@sapphi-red/web-noise-suppressor": "^0.3.5",
    "@shiguredo/virtual-background": "^2023.2.0",
    "@traptitech/traq": "^3.26.1-1",
    "@traptitech/traq-markdown-it": "^7.0.2",
    "@vueuse/core": "^14.1.0",
    "autosize": "^6.0.1",
    "browser-image-compression": "^2.0.2",
    "cropperjs": "^1.6.2",
    "deepmerge": "^4.3.1",
    "dequal": "^2.0.3",
    "firebase": "^12.7.0",
    "highlight.js": "^11.11.1",
    "idb-keyval": "^6.2.2",
    "livekit-client": "^2.17.0",
    "mitt": "^3.0.0",
    "party-js": "^2.2.0",
    "sortablejs": "^1.15.6",
    "text-field-edit": "^4.1.1",
    "throttle-debounce": "^5.0.2",
    "ts-pattern": "^5.9.0",
    "turndown": "^7.2.2",
    "turndown-plugin-gfm": "^1.0.2",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4",
    "vue-select": "^4.0.0-beta.6",
    "vue-slider-component": "^4.0.0-beta.9",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.3",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/autosize": "^4.0.3",
    "@types/dom-screen-wake-lock": "^1.0.3",
    "@types/katex": "^0.16.8",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^25.0.3",
    "@types/punycode": "^2.1.4",
    "@types/serviceworker": "^0.0.182",
    "@types/sortablejs": "^1.15.9",
    "@types/throttle-debounce": "^5.0.2",
    "@types/turndown": "^5.0.6",
    "@types/vue-select": "^3.16.8",
    "@types/webappsec-credential-management": "^0.6.9",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@typescript/lib-dom": "npm:@types/web@^0.0.72",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.24",
    "axios": "^1.13.4",
    "browserslist": "^4.28.1",
    "cypress": "^15.8.1",
    "esbuild": "^0.27.2",
    "esbuild-plugin-browserslist": "^2.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-vue": "^10.6.2",
    "fake-indexeddb": "^6.2.5",
    "fonteditor-core": "^2.6.3",
    "jsdom": "^27.4.0",
    "patch-package": "^8.0.1",
    "pinia": "^3.0.1",
    "postcss": "^8.5.3",
    "prettier": "^3.8.1",
    "rollup-plugin-brotli": "^3.1.0",
    "sass": "^1.97.3",
    "start-server-and-test": "^2.1.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^4.0.6",
    "vitest-github-actions-reporter": "^0.11.1",
    "vue-tsc": "^3.2.4",
    "workbox-cacheable-response": "^7.4.0",
    "workbox-core": "^7.4.0",
    "workbox-expiration": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.4.0"
  },
  "engines": {
    "node": "^22.14.0",
    "npm": "^10.9.0"
  }
}
