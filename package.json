{
  "name": "traq_s-ui",
  "version": "3.13.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with-font": "npm run gen-fonts && npm run build",
    "build:may-with-font": "MAY_SKIP_FONT_GEN=1 npm run gen-fonts && npm run build",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest run --coverage",
    "test:unit:watch": "vitest -w",
    "test:e2e": "npm run build && npm run test:e2e:only-run",
    "test:e2e-headless": "npm run build:may-with-font && npm run test:e2e:only-run-headless",
    "test:e2e:only-run": "start-server-and-test preview 4173 'cypress open'",
    "test:e2e:only-run-headless": "start-server-and-test preview 4173 'cypress run --headless'",
    "lint": "eslint --fix --cache .",
    "lint:no-fix": "eslint --cache .",
    "type-check": "npm run type-check:src && npm run type-check:test",
    "type-check:src": "vue-tsc --noEmit",
    "type-check:test": "tsc --noEmit --project tests/unit/tsconfig.json && tsc --noEmit --project tests/e2e/tsconfig.json",
    "gen-fonts": "node build/gen-mplus.mjs",
    "gen-unicode_emojis": "node build/gen-unicode_emojis.mjs",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@mdi/js": "^6.5.95",
    "@sapphi-red/dtln-web": "^0.1.3",
    "@traptitech/traq": "^3.9.1-4",
    "@traptitech/traq-markdown-it": "^6.1.6",
    "autosize": "^5.0.1",
    "cropperjs": "^1.5.12",
    "deepmerge": "^4.2.2",
    "dequal": "^2.0.2",
    "direct-vuex": "^0.12.1",
    "firebase": "^9.6.8",
    "highlight.js": "^11.4.0",
    "idb-keyval": "^6.1.0",
    "klona": "^2.0.5",
    "mitt": "^3.0.0",
    "pica": "^9.0.1",
    "shvl": "^2.0.3",
    "skyway-js": "^4.4.3",
    "text-field-edit": "^3.1.9001",
    "throttle-debounce": "^3.0.1",
    "turndown": "^7.1.1",
    "turndown-plugin-gfm": "^1.0.2",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vue-slider-component": "^4.0.0-beta.5",
    "vuex": "^4.0.2",
    "vuex-persist": "^3.1.3",
    "zod": "^3.13.4"
  },
  "devDependencies": {
    "@types/autosize": "^4.0.1",
    "@types/dom-screen-wake-lock": "^1.0.0",
    "@types/katex": "^0.11.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^16.11.26",
    "@types/pica": "^9.0.0",
    "@types/punycode": "^2.1.0",
    "@types/serviceworker": "^0.0.37",
    "@types/throttle-debounce": "^2.1.0",
    "@types/turndown": "^5.0.1",
    "@types/webappsec-credential-management": "^0.6.2",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@typescript/lib-dom": "npm:@types/web@^0.0.47",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/test-utils": "^2.0.0-rc.18",
    "autoprefixer": "^10.4.2",
    "axios": "^0.26.1",
    "browserslist": "^4.20.0",
    "c8": "^7.11.0",
    "cypress": "^9.5.1",
    "esbuild": "^0.13.15",
    "esbuild-plugin-browserslist": "^0.3.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "fonteditor-core": "^2.1.8",
    "jsdom": "^19.0.0",
    "patch-package": "^6.4.7",
    "postcss": "^8.4.8",
    "prettier": "^2.5.1",
    "rollup-plugin-brotli": "^3.1.0",
    "sass": "^1.49.9",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vite-plugin-pwa": "^0.11.13",
    "vite-plugin-static-copy": "^0.4.0",
    "vite-svg-loader": "^3.1.2",
    "vitest": "^0.5.9",
    "vitest-github-actions-reporter": "^0.1.7",
    "vue-tsc": "^0.32.1",
    "workbox-cacheable-response": "^6.5.1",
    "workbox-core": "^6.5.1",
    "workbox-precaching": "^6.5.1",
    "workbox-routing": "^6.5.1",
    "workbox-strategies": "^6.5.1"
  },
  "engines": {
    "node": "^16.13.2",
    "npm": "^7.19.1"
  }
}
